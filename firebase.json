{
	"hosting": {
		"predeploy": "pushd next-app && npm run build && npm run export && popd",
		"public": "next-app/out",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites": [
			{
				"source": "/api",
				"function": "launchesApi"
			},
			{
				"source": "/calendar",
				"function": "launchesCal"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}
